# 网速测试应用环境变量配置示例
# 复制此文件为 config.env 并根据需要修改

# 应用配置
BIND_ADDRESS=0.0.0.0
PORT=8080
BASE_URL=

# TLS配置
ENABLE_TLS=false
ENABLE_HTTP2=false
TLS_CERT_FILE=
TLS_KEY_FILE=

# 日志配置
LOG_LEVEL=info

# 性能配置
CHUNK_SIZE=4
MAX_CHUNK_SIZE=100
HTTP_TIMEOUT=30s

# 安全配置
ALLOWED_ORIGINS=*
ALLOWED_METHODS=GET,POST,OPTIONS,HEAD
ALLOWED_HEADERS=*
